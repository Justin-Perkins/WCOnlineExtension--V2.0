{
  "name": "SNHU MyWCOnline Form Editor",
  "description": "Removes unnecessary options and auto-fills fields in the \"Off-Schedule Report\" form",
  "author": "Justin Perkins",
  "version": "1.0.0",

  "manifest_version": 3,

  "icons": {
  "16": "/images/icon16.png" ,
  "48": "/images/icon48.png" ,
  "128": "/images/icon128.png"
  },

  "permissions": [
    "storage"
  ],

  "action": {
    "default_popup": "popup/popup.html"
  },

  "content_scripts": [{
    "matches": ["https://snhu.mywconline.com/schedule/*"],
    "js": ["scripts/formManager.js"]
    }]
}